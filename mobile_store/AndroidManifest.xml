        android:label="@string/app_name"
        android:theme="@style/MobileStoreTheme" >
        <activity
            android:name="rs.gopro.mobile_store.ui.HomeScreenActivity"
            android:label="@string/app_name"
            android:launchMode="singleTop" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <service android:name="rs.gopro.mobile_store.ws.NavisionSyncService" />

        <activity
            android:name="rs.gopro.mobile_store.ui.LoginActivity"
            android:exported="false"
            android:label="@string/login_title" >
